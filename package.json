{
  "name": "@mainframework/api-reqpuest-provider-worker-hook",
  "version": "2.2.8",
  "description": "Package to handle api requests with a hook, provider and web worker",
  "type": "module",
  "main": "./dist/index.js",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "default": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "types": "./dist/index.d.ts",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/TerrySlack/mainframework-api-hook-provider-worker.git"
  },
  "bugs": {
    "url": "https://github.com/TerrySlack/mainframework-api-hook-provider-worker/issues"
  },
  "homepage": "https://github.com/TerrySlack/mainframework-api-hook-provider-worker#readme",
  "scripts": {
    "husky-setup": "npx husky init",
    "husky": "husky",
    "prepare": "husky",
    "clean": "rimraf dist",
    "lint": "eslint --ext 'ts,tsx' --max-warnings=0 --fix",
    "lint-staged": "lint-staged",
    "prebuild": "yarn husky && yarn clean",
    "build": "yarn prebuild && yarn build:esm",
    "build:esm": "tsc",
    "test": "jest --config jestconfig.json"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint --max-warnings=0"
    ],
    "*.{js,jsx,ts,tsx,json,css,md}": [
      "prettier --write"
    ]
  },
  "peerDependencies": {
    "react": ">=19"
  },
  "peerDependenciesMeta": {
    "react": {
      "optional": false
    }
  },
  "files": [
    "dist",
    "LICENSE",
    "README.md"
  ],
  "keywords": [
    "react",
    "typescript",
    "webworker",
    "store"
  ],
  "author": "Terry Slack",
  "license": "MIT",
  "devDependencies": {
    "@mainframework/is-deep-equal": "^1.0.9",
    "@testing-library/react": "^16.3.2",
    "@types/jest": "^30.0.0",
    "@types/react": "^19.2.13",
    "eslint": "^10.0.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^7.0.1",
    "husky": "^9.1.7",
    "jest": "^30.2.0",
    "jest-canvas-mock": "^2.5.2",
    "jest-environment-jsdom": "^30.2.0",
    "lint-staged": "^16.2.7",
    "prettier": "^3.8.1",
    "react": "^19.2.4",
    "rimraf": "^6.1.2",
    "ts-jest": "^29.4.6",
    "tslib": "^2.8.1",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.54.0"
  }
}
